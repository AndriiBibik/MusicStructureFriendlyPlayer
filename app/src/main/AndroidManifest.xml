<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.musicstructurefriendlyplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_title"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="@string/folders">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AllMusic"
            android:label="@string/all_music" />
        <activity
            android:name=".NowPlaying"
            android:label="@string/now_playing" />
        <activity
            android:name=".MusicStore"
            android:label="@string/music_store" />
        <activity
            android:name=".MeditationSpaceBase"
            android:label="@string/meditation_spacebase"/>
    </application>
</manifest>